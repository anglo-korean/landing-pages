        "dev": "cross-env NODE_OPTIONS=--openssl-legacy-provider preact watch",
        "lint": "eslint src",
        "test": "jest"
    },
    "eslintConfig": {
        "extends": "preact",
        "ignorePatterns": [
            "build/"
        ]
    },
    "devDependencies": {
        "cross-env": "^7.0.3",
        "enzyme": "^3.11.0",
        "enzyme-adapter-preact-pure": "^4.0.0",
        "eslint": "^8.30.0",
        "eslint-config-preact": "^1.3.0",
        "gh-pages": "^5.0.0",
        "preact-cli": "^3.4.2",
        "preact-svg-loader": "^0.2.1",
        "sirv-cli": "^2.0.2"
    },
    "dependencies": {
        "@material-ui/core": "^4.12.4",
        "preact": "^10.11.3",
        "preact-render-to-string": "^5.2.6",
        "preact-router": "^3.2.1",
        "redaxios": "^0.5.1"
    }
}
