{
    "private": true,
    "name": "Anko Investor",
    "version": "0.0.0",
    "homepage": "https://anglo-korean.github.io/landing-pages",
    "license": "MIT",
    "scripts": {
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build",
        "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider preact build",
